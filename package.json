{
  "name": "flamsdk",
  "version": "2.0.1",
  "description": "flamSdk for saas",
  "author": "bucharitesh",
  "license": "MIT",
  "module": "dist/FlamSaasSDK.min.esm.js",
  "main": "dist/FlamSaasSDK.min.js",
  "files": [],
  "keywords": [],
  "scripts": {
    "clean": "rm -rf dist && rm -rf build",
    "start": "npm run clean && rollup -c --watch",
    "build": "npm run clean && rollup -c --prod && cp -rf dist build",
    "test": "cross-env BABEL_ENV=test mocha --require @babel/register --require jsdom-global/register test/**/*.test.js --exit",
    "ci:test": "nyc npm run test -- --forbid-only --reporter mocha-junit-reporter",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prepare": "husky install",
    "jsdocs": "jsdoc --configure .jsdoc.json --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react-hot-toast": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "babel-plugin-istanbul": "^6.0.0",
    "browserstack-local": "^1.4.8",
    "chromedriver": "^103.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "es-check": "^6.2.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.24.2",
    "expect.js": "^0.3.1",
    "husky": "^8.0.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.8",
    "jsdoc-to-markdown": "8.0.0",
    "jsdom": "^20.0.3",
    "jsdom-global": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.1.2",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "nyc": "^15.1.0",
    "oidc-provider": "^8.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "proxyquire": "^2.1.1",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^2.6.1",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1",
    "selenium-webdriver": "^3.6.0",
    "sinon": "^15.0.1",
    "wait-on": "^7.0.1",
    "yargs": "^17.2.1"
  },
  "overrides": {
    "lodash": "^4.17.21"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "set-value": "^2.0.1",
    "acorn": "^7.1.1",
    "serialize-javascript": "^2.1.1",
    "kind-of": "^6.0.3"
  }
}
